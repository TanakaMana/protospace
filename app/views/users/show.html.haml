.container.user-page
  %header.user-nav
    .media
      .media-left
        = link_to '#'
        %img.media-object{ alt: "64x64", data_holder_rendered: "true", data_src: "holder.js/64x64", style: "width: 64px; height: 64px;"}/
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
          = current_user.member
        .row
          .col-md-6
            %h4 Profile
            %p
              = current_user.profile
          .col-md-6
            %h4 Works
            %p
              = current_user.work
.container.proto-list
  .row
    .col-sm-4.col-md-3.proto-content
      = render partial: "prototypes/prototype"
